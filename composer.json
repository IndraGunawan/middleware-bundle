{
    "name": "indragunawan/middleware-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Indra Gunawan",
            "email": "hello@indra.my.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Indragunawan\\MiddlewareBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indragunawan\\MiddlewareBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "doctrine/common": "^2.4",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
